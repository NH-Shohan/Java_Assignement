<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
        PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
        "http://java.sun.com/dtd/web-app_2_3.dtd">
<web-app>
    <display-name>Forms With Table</display-name>
    <description>
        Admin: Nahim Hossain Shohan
        Email: nh.shohan6@gmail.com
    </description>

    <servlet>
        <servlet-name>Forms</servlet-name>
        <servlet-class>FormsServlet</servlet-class>
        <load-on-startup>5</load-on-startup>
    </servlet>

    <servlet>
        <servlet-name>Home</servlet-name>
        <servlet-class>HomeServlet</servlet-class>
        <load-on-startup>5</load-on-startup>
    </servlet>

    <servlet-mapping>
        <servlet-name>Forms</servlet-name>
        <url-pattern>/forms</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>Home</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
</web-app>